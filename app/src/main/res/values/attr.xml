<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomWithFloatingNavView">
        <!-- Elevation for the BottomAppBar. -->
        <attr name="elevation" />
        <!-- The margin between the semi-circular cradle for the fab and the fab. -->
        <attr name="fabCradleMargin" format="dimension" />
        <!-- The radius of the rounded corners on each side of the cradle摇篮. -->
        <attr name="fabCradleRoundedCornerRadius" format="dimension" />
        <!-- The vertical offset between the fab from the cradle. -->
        <attr name="fabCradleVerticalOffset" format="dimension" />
        <attr name="fabCradleSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="IconTextView">
        <attr name="iconPadding" format="dimension" />
        <attr name="icon" format="reference" />
        <attr name="iconUrl" format="string" />
        <attr name="text" format="reference" />
        <attr name="textStr" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="ivlayout" format="flags">
            <flag name="tv_rtl" value="1" />
            <flag name="tv_ltr" value="2" />
            <flag name="tv_ttb" value="3" />
            <flag name="tv_btt" value="4" />
        </attr>
    </declare-styleable>

</resources>